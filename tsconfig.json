        "target": "es2022",
        "outDir": "./dist",
        "module": "esnext",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "allowJs": true,
        "declaration": true
